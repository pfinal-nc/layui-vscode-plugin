{
	"name": "layui-snippets-pfinal",
	"displayName": "layui-vscode-plugin",
	"description": "开发中常用的 layui 框架自动补全",
	"keywords": ["vscode", "plugin", "layui","pfinal","pfinalclub"],
	"publisher": "PFinal-nc",
	"version": "0.0.6",
	"engines": {
		"vscode": "^1.52.0"
	},
	"categories": [
		"Snippets"
	],
	"icon": "images/icon.png",
	"contributes": {
		"configuration": {
            "type": "Layui-Snippets",
            "title": "vscode-plugin-layui-snippets",
            "properties": {
                "vscodePluginDemo.yourName": {
                    "type": "string",
                    "default": "guest",
                    "description": "PFinal-nc"
                }
            }
        },
		"snippets": [
			{
                "language": "html",
                "path": "./snippets/snippets.json"
			},
			{
                "language": "php",
                "path": "./snippets/snippets.json"
			},
			{
                "language": "javascript",
                "path": "./snippets/snippets.json"
			}
		]
	}
}
