{
	"name": "layui-snippets-pfinal",
	"displayName": "layui-vscode-plugin",
	"description": "开发中常用的 layui 框架自动补全 - 支持最新 2.13.2 版本，包含55+代码片段，新增Form核心方法和Layer完整示例",
	"keywords": ["vscode", "plugin", "layui", "pfinal", "pfinalclub", "snippets", "2.13.2", "component", "tabs", "form", "layer", "render", "verify"],
	"publisher": "PFinal-nc",
	"version": "0.3.1",
	"engines": {
		"vscode": "^1.52.0"
	},
	"categories": [
		"Snippets"
	],
	"icon": "images/icon.png",
	"contributes": {
		"configuration": {
            "type": "Layui-Snippets",
            "title": "vscode-plugin-layui-snippets",
            "properties": {
                "vscodePluginDemo.yourName": {
                    "type": "string",
                    "default": "guest",
                    "description": "PFinal-nc"
                }
            }
        },
		"snippets": [
			{
                "language": "html",
                "path": "./snippets/snippets.json"
			},
			{
                "language": "php",
                "path": "./snippets/snippets.json"
			},
			{
                "language": "javascript",
                "path": "./snippets/snippets.json"
			}
		]
	}
}
